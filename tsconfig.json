{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"strict": true,
		"types": [
			"vite/client"
		],
		"baseUrl": "./",
		"paths": {
			"~/*": [
				"./src/*"
			]
		}
	}
}
